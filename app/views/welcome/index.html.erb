<%= button_to "Create New User", register_path, method: :get %>
<% if !session[:user_id] %>
    <section class='existing-users'>
        <p>Existing Users: </p>
        <% @users.each do |user| %>
            <li>
                <%= 
                    #link_to user.email, user_path(user.id) 
                    user.email
                %>
            </li>
        <% end %>
    </section>
<% end %>